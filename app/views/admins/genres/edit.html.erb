<div class="container">
    <div class="row">
        <h3>ジャンル編集</h3>
    </div>
    
    <div class-"row">
         <%= form_with model: @genre, url: admins_genre_path(@genre.id), local:true do |f| %>
        <div class="form-group">
            <label for="name">ジャンル名</label>
            <%= f.text_field :name %>
            <%= f.submit "編集する", class:"btn btn-success" %>
    </div>
    <% end %>
</div>